{
    "name": "lpc_frontend",
    "version": "0.1.0",
    "publishConfig": {
        "registry": "https://nexus.lepotcommuntest.fr/repository/npm-private/"
    },
    "scripts": {
        "dev": "NODE_ENV=development NODE_OPTIONS='--inspect' node ./src/server.js",
        "dev-win": "set NODE_ENV=development&& set NODE_OPTIONS='--inspect'&& node ./src/server.js",
        "build": "next build",
        "start": "NODE_ENV=integration node ./src/server.js",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "lint-win": "eslint */**/*.{js,ts,tsx} --quiet --fix",
        "storybook": "start-storybook -s ./public -p 6006",
        "build-storybook": "build-storybook",
        "test": "jest",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "@babel/preset-env": "^7.11.0",
        "@n8tb1t/use-scroll-position": "^1.0.47",
        "@next/bundle-analyzer": "^9.5.2",
        "@types/next": "^9.0.0",
        "@types/yup": "^0.29.3",
        "@zeit/next-sass": "^1.0.1",
        "axios": "^0.20.0",
        "body-parser": "^1.19.0",
        "core-js": "^2.0.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "formik": "^2.1.5",
        "http-proxy-middleware": "^1.0.5",
        "i18next": "^19.7.0",
        "moment": "^2.27.0",
        "next": "9.4.4",
        "next-i18next": "^4.5.0",
        "next-routes": "^1.4.2",
        "node-sass": "^4.14.1",
        "react": "16.13.1",
        "react-collapsible": "^2.8.0",
        "react-dom": "16.13.1",
        "react-dropdown": "^1.7.0",
        "react-i18next": "^11.7.0",
        "react-lottie": "^1.2.3",
        "react-slick": "^0.27.9",
        "slick-carousel": "^1.8.1",
        "ts-loader": "^8.0.1",
        "yup": "^0.29.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-docs": "^5.3.19",
        "@storybook/addon-links": "^5.3.19",
        "@storybook/addon-storyshots": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^5.3.19",
        "@testing-library/react": "^10.4.8",
        "@types/node": "^14.0.23",
        "@types/react": "^16.9.43",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.10.1",
        "@zeit/next-css": "^1.0.1",
        "@zeplin/cli": "^1.0.4",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.1.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.3.0",
        "jest-css-modules-transform": "^4.0.1",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5",
        "react-test-renderer": "^16.13.1",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.2",
        "typescript": "^3.9.7"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm test && npm run build"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
